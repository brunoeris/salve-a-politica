<p id="notice"><%= notice %></p>

<h1>Judicial Processes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @judicial_processes.each do |judicial_process| %>
      <tr>
        <td><%= link_to 'Show', judicial_process %></td>
        <td><%= link_to 'Edit', edit_judicial_process_path(judicial_process) %></td>
        <td><%= link_to 'Destroy', judicial_process, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Judicial Process', new_judicial_process_path %>
